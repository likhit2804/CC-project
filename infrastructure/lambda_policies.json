{
  "SubmitterPolicy": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "s3:PutObject",
          "s3:PutObjectAcl"
        ],
        "Resource": [
          "arn:aws:s3:::REPLACE_BUCKET/*"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
          "dynamodb:PutItem",
          "dynamodb:GetItem"
        ],
        "Resource": [
          "arn:aws:dynamodb:REPLACE_REGION:REPLACE_ACCOUNT:table/REPLACE_TABLE"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
          "sqs:SendMessage"
        ],
        "Resource": [
          "arn:aws:sqs:REPLACE_REGION:REPLACE_ACCOUNT:REPLACE_QUEUE"
        ]
      }
    ]
  },
  "WorkerPolicy": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "s3:GetObject"
        ],
        "Resource": [
          "arn:aws:s3:::REPLACE_BUCKET/*"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
          "dynamodb:GetItem",
          "dynamodb:UpdateItem"
        ],
        "Resource": [
          "arn:aws:dynamodb:REPLACE_REGION:REPLACE_ACCOUNT:table/REPLACE_TABLE"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
          "sqs:ReceiveMessage",
          "sqs:DeleteMessage"
        ],
        "Resource": [
          "arn:aws:sqs:REPLACE_REGION:REPLACE_ACCOUNT:REPLACE_QUEUE"
        ]
      }
    ]
  }
}